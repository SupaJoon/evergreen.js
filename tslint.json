{
    "extends": ["tslint:recommended"],
    "linterOptions": {
      "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts",
        "coverage/lcov-report/*.js",
        "scripts/*"
      ]
    },
    "rules": {
      "no-console": false,
      "object-literal-sort-keys": false,
      "object-literal-shorthand": false
    }
}